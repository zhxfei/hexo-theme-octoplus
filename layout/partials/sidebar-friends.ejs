<% if (theme.friends !== undefined) { %> 
    <div class="sidebar-friends-title">
        <h3>Friends' Links</h3>
    </div>
    <div class="sidebar-friends-content">
        <ul>            
            <% for (var i in theme.friends){ %>
                <li>
                    <a target="_blank" href="<%- url_for(theme.friends[i]) %>"> ->     <%= i %></a>
                </li>
              <%}%>  
        </ul>
    </div>
    <hr class="profile-hr-dotted">

<% } %>
